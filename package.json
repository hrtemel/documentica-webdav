{
  "name": "@documentica/webdav",
  "version": "0.1.8",
  "description": "Documentica Webdav Client",
  "scripts": {
    "build": "npm run build:tsc",
    "build:tsc": "tsc -d",
    "prebuild": "npm run prebuild:clean && npm run prebuild:version",
    "prebuild:clean": "rimraf es",
    "prebuild:version": "npm version patch",
    "postbuild": "npm run postbuild:copy",
    "postbuild:copy": "copy-files-from-to"
  },
  "main": "index.js",
  "engineStrict": false,
  "engines": {
    "npm": ">=2.14.7",
    "node": ">=6"
  },
  "peerDependencies": {
    "lodash.isequal": "^4.0.0",
    "xmldom": "^0.3.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.0.0",
    "xmldom": "^0.3.0"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.0.0",
    "copy-files-from-to": "^2.3.0",
    "edit-json-file": "^1.4.0",
    "eslint": "^5.14.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrtemel/documentica-webdav.git"
  },
  "author": "Haydar RÄ±dvan TEMEL <hrtemel@gmail.com>",
  "keywords": [
    "documentica",
    "webdav"
  ],
  "license": "Apache-2.0",
  "url": "https://github.com/hrtemel/documentica-webdav.git",
  "copyFiles": [
    {
      "from": "package.json",
      "to": "es/package.json"
    },
    {
      "from": "README.md",
      "to": "es/README.md"
    }
  ]
}
